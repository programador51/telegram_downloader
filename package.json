  "type": "module",
  "scripts": {
    "dev": "vite",
    "buildAddOn": "tsc && vite build --config viteAddOn.config.ts",
    "buildAddOnBg": "tsc && vite build --config viteAddOnBg.config.ts",
    "build": "tsc && vite build",
    "create": "npm run build && npm run buildAddOn && npm run buildAddOnBg",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:firefox": "web-ext run --source-dir ./dist",
    "semantic-release": "semantic-release --branches main",
    "commit": "cz"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "bootswatch": "^5.3.3",
    "byte-size": "^8.1.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "mime-types": "^2.1.35",
